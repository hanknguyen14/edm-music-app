[declarations]

[ignore]
.*/tests/*
.*/.history/*

[include]

[libs]

[lints]

[options]
module.file_ext=.js
module.file_ext=.jsx
module.file_ext=.json
module.file_ext=.css
module.file_ext=.scss

module.name_mapper='^assets\(.*\)$' -> '<PROJECT_ROOT>/src/assets/\1'
module.name_mapper='^config\(.*\)$' -> '<PROJECT_ROOT>/src/config/\1'
module.name_mapper='^components\(.*\)$' -> '<PROJECT_ROOT>/src/components/\1'
module.name_mapper='^router\(.*\)$' -> '<PROJECT_ROOT>/src/router/\1'
module.name_mapper='^services\(.*\)$' -> '<PROJECT_ROOT>/src/services/\1'
module.name_mapper='^util\(.*\)$' -> '<PROJECT_ROOT>/src/util/\1'

module.ignore_non_literal_requires=true

[strict]
